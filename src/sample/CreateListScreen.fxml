<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CreateListScreenController">
   <children>
      <Button fx:id="backButton" layoutX="93.0" layoutY="92.0" mnemonicParsing="false" onAction="#goBack" prefHeight="35.0" prefWidth="130.0" text="&lt; Go Back" />
      <Label alignment="CENTER_RIGHT" layoutX="138.0" layoutY="249.0" prefHeight="27.0" prefWidth="144.0" text="Name : ">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <Label alignment="CENTER_RIGHT" layoutX="138.0" layoutY="310.0" prefHeight="27.0" prefWidth="144.0" text="Description : ">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <TextField fx:id="nameField" layoutX="311.0" layoutY="250.0" prefHeight="26.0" prefWidth="232.0" />
      <TextArea fx:id="descriptionBox" layoutX="311.0" layoutY="310.0" prefHeight="110.0" prefWidth="323.0" />
      <MenuButton fx:id="privacyButton" layoutX="159.0" layoutY="452.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="130.0" text="Public">
        <items>
          <MenuItem fx:id="publicMenu" mnemonicParsing="false" onAction="#selectPrivacy" text="Public" />
          <MenuItem fx:id="privateMenu" mnemonicParsing="false" onAction="#selectPrivacy" text="Private" />
        </items>
      </MenuButton>
      <Button fx:id="createButton" layoutX="421.0" layoutY="518.0" mnemonicParsing="false" onAction="#create" prefHeight="26.0" prefWidth="149.0" text="Create" />
      <Label fx:id="errorLabel" layoutX="457.0" layoutY="460.0" prefHeight="28.0" prefWidth="166.0" text="Label" />
      <Label layoutX="76.0" layoutY="452.0" prefHeight="26.0" prefWidth="70.0" text="Privacy: " textFill="#1931bd">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
