<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ListDetailsScreenController">
   <children>
      <Button fx:id="backButton" layoutX="89.0" layoutY="58.0" mnemonicParsing="false" onAction="#goBack" prefHeight="26.0" prefWidth="119.0" text="&lt; Go Back" />
      <Label fx:id="nameLabel" alignment="CENTER" layoutX="219.0" layoutY="124.0" prefHeight="56.0" prefWidth="362.0" text="Label">
         <font>
            <Font name="System Bold" size="27.0" />
         </font>
      </Label>
      <TextArea fx:id="descriptionBox" editable="false" layoutX="200.0" layoutY="245.0" prefHeight="92.0" prefWidth="406.0" />
      <Label alignment="CENTER" layoutX="222.0" layoutY="203.0" prefHeight="56.0" prefWidth="362.0" text="Description:">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <ListView fx:id="restaurants" layoutX="188.0" layoutY="362.0" prefHeight="260.0" prefWidth="430.0" />
      <Button fx:id="privacyButton" layoutX="618.0" layoutY="277.0" mnemonicParsing="false" onAction="#changePrivacy" prefHeight="26.0" prefWidth="119.0" text="Make Private" />
      <Button fx:id="removeButton" layoutX="403.0" layoutY="641.0" mnemonicParsing="false" onAction="#remove" prefHeight="26.0" prefWidth="119.0" text="Remove" />
      <Button fx:id="detailsButton" layoutX="271.0" layoutY="641.0" mnemonicParsing="false" onAction="#showDetails" prefHeight="26.0" prefWidth="119.0" text="Show Details" />
   </children>
</AnchorPane>
